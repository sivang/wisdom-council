{
  "app": "python3 app.py",
  "watch": {
    "include": ["**"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__pycache__",
      "**/*.pyc",
      ".git",
      ".venv"
    ]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk/core:stackRelativeExports": true
  }
}